					columnsXL="2" columnsL="1" columnsM="1" singleContainerFullSize="false">
					<f:content>
						
						<Label text="Description"/>
						<Text id="Descript" text=""/>
						<Label text="Notification Type"/>
						<Text id="NotifType" text=""/>
						<Label text="Notification Date"/>
						<Text id="Notifdate" text=""/>
						<Label text="Equipment Number"/>
						<Text id="Equipment" text=""/>
						<Label text="Equipment Description"/>
						<Text id="Equidescr" text=""/>
						<Label text="Functional Location"/>
						<Text id="Funcloc" text=""/>
						<Label text="FunLoc Description"/>
						<Text id="Funcldescr" text=""/>
						<Label text="Priority"/>
						<Text id="Priority" text=""/>
						<Label text="Priority Type"/>
						<Text id="Priotype" text=""/>
						<Label text="System Status"/>
						<Text id="SStatus" text=""/>
					</f:content>
				</f:SimpleForm>
			</l:content>
		</l:Grid>
		<buttons>
			<Button text="Close" press="onCancelDialog"></Button>
		</buttons>
	</Dialog>
</core:FragmentDefinition>