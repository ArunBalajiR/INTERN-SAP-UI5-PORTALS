<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form">
	<Dialog id="notificationDetailDialog" title="Notification Detail">
		<l:Grid defaultSpan="L12 M12 S12" width="auto" >
			<l:content>
				<f:SimpleForm id="NotificationDetail" width="31rem" editable="true" layout="ResponsiveGridLayout" title="" labelSpanXL="4"
